{
    "name": "digistorm/school-easy-pay",
    "type": "library",
    "description": "School EasyPay PHP SDK",
    "keywords": [
        "digistorm",
        "school-easy-pay"
    ],
    "homepage": "https://github.com/digistorm/school-easy-pay",
    "license": "MIT",
    "authors": [
        {
            "name": "Digistorm",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "guzzlehttp/guzzle": "^6.3",
        "myclabs/php-enum": "^1.7"
    },
    "require-dev": {
        "phpunit/phpunit": ">=7.0",
        "rector/rector": "^1.2",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Digistorm\\SchoolEasyPay\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Digistorm\\SchoolEasyPay\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs src tests",
        "fix-style": "phpcbf src tests"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true,
        "platform": {
            "php": "8.0.30"
        }
    }
}
